{
  "name": "@mgdn/bvid",
  "version": "0.0.1",
  "description": "bilibili bvid",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "default": "./dist/index.d.ts"
      },
      "default": {
        "import": "./dist/index.mjs",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "vitest run",
    "prepare": "husky install",
    "test:dev": "vitest",
    "test-cover": "vitest run --coverage",
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm test-cover && pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/bvid.git"
  },
  "keywords": [
    "bvid",
    "bilibili"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/bvid/issues"
  },
  "homepage": "https://github.com/magicdawn/bvid#readme",
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.2",
    "@swc/core": "^1.3.88",
    "@types/node": "^20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitest/coverage-v8": "^0.34.5",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  }
}
