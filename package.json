{
  "name": "@mgdn/bvid",
  "version": "0.2.1",
  "description": "bilibili bvid",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "vitest run",
    "prepare": "husky install",
    "test:dev": "vitest",
    "test-cover": "vitest run --coverage",
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm test-cover && pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/bvid.git"
  },
  "keywords": [
    "bvid",
    "bilibili"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/bvid/issues"
  },
  "homepage": "https://github.com/magicdawn/bvid#readme",
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.3",
    "@swc/core": "^1.6.0",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.3.0+sha512.ee7b93e0c2bd11409c6424f92b866f31d3ea1bef5fbe47d3c7500cdc3c9668833d2e55681ad66df5b640c61fa9dc25d546efa54d76d7f8bf54b13614ac293631"
}
